local ImGui = loadstring(game:HttpGet("https://raw.githubusercontent.com/antixcat/SynWare-Library/refs/heads/main/CloudLib", true))()

local Window = ImGui:Begin({
    Name = "victim menu",
    Width = 530,
    Height = 400
})

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local localPlayer = Players.LocalPlayer
local attacking = false
local walkflinging = false

local function enablenoclip()
    local character = localPlayer.Character or localPlayer.CharacterAdded:Wait()
    for _, part in pairs(character:GetDescendants()) do
        if part:IsA("BasePart") and part.CanCollide then
            part.CanCollide = false
        end
    end
    RunService.Stepped:Connect(function()
        for _, part in pairs(character:GetDescendants()) do
            if part:IsA("BasePart") and part.CanCollide then
                part.CanCollide = false
            end
        end
    end)
end

local function disablenoclipxd()
    local character = localPlayer.Character or localPlayer.CharacterAdded:Wait()
    for _, part in pairs(character:GetDescendants()) do
        if part:IsA("BasePart") then
            part.CanCollide = true
        end
    end
end

local function attackPlayer(targetPlayer)
    if not targetPlayer then return end

    attacking = true
    walkflinging = true
    enablenoclip()
    coroutine.wrap(function()
        local character = localPlayer.Character
        if not character or not character:FindFirstChild("HumanoidRootPart") then return end

        local humanoidRootPart = character.HumanoidRootPart
        local movel = 0.1

        while attacking and walkflinging do
            RunService.Heartbeat:Wait()

            if targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
                local targetPosition = targetPlayer.Character.HumanoidRootPart.Position
                humanoidRootPart.CFrame = CFrame.new(targetPosition)
                local velocity = humanoidRootPart.Velocity
                humanoidRootPart.Velocity = velocity * 10000 + Vector3.new(0, 10000, 0)
                RunService.RenderStepped:Wait()
                humanoidRootPart.CFrame = CFrame.new(targetPosition)
                humanoidRootPart.Velocity = velocity
                RunService.Stepped:Wait()
                humanoidRootPart.Velocity = velocity + Vector3.new(0, movel, 0)
                movel = movel * -1
            else
                break
            end
        end
        walkflinging = false
        disablenoclipxd()
    end)()
end

Window:Text("playerlist xd")

for _, player in ipairs(Players:GetPlayers()) do
    if player ~= localPlayer then
        Window:Button(player.Name):Connect(function()
            attackPlayer(player)
        end)
    end
end

Window:End()
